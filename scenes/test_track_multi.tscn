[sub_resource type="Shader" id="Shader_checker"]
code = "shader_type spatial;

uniform vec3 color_a : source_color = vec3(0.9, 0.9, 0.9);
uniform vec3 color_b : source_color = vec3(0.7, 0.7, 0.7);
uniform float uv_scale = 50.0;

void fragment() {
	vec2 uv = UV * uv_scale;
	float checker = mod(floor(uv.x) + floor(uv.y), 2.0);
	ALBEDO = mix(color_a, color_b, checker);
	ROUGHNESS = 0.8;
}"

